<Resource>
  <Finder class="de.dante.util.resource.FileFinder"
          default="default">
    <tex>
      <path property="extex.texinputs"/>
      <path property="texinputs"/>
      <path>.</path>
      <extension></extension>
      <extension>.tex</extension>
    </tex>
    <fmt>
      <path property="extex.texinputs"/>
      <path property="texinputs"/>
      <path>.</path>
      <extension></extension>
      <extension>.fmt</extension>
    </fmt>
    <default>
      <path property="extex.texinputs"/>
      <path property="texinputs"/>
      <path>.</path>
      <extension></extension>
    </default>
  </Finder>

  <Finder class="de.dante.util.resource.LsrFinder"
          default="default">
    <path property="extex.texinputs"/>
    <path property="texinputs"/>

    <tex><extension>.tex</extension></tex>
    <fmt><extension>.fmt</extension></fmt>
    <default><extension/></default>
  </Finder>

  <Finder class="de.dante.extex.main.ResourceFinderImpl" default="default">
    <tex>
      <path property="extex.texinputs"/>
      <path property="texinputs"/>
      <path>.</path>
      <extension></extension>
      <extension>.tex</extension>
    </tex>
    <fmt>
      <path property="extex.texinputs"/>
      <path property="texinputs"/>
      <path>.</path>
      <extension></extension>
      <extension>.fmt</extension>
    </fmt>
    <default>
      <path property="extex.texinputs"/>
      <path property="texinputs"/>
      <path>.</path>
      <extension/>
    </default>
  </Finder>
</Resource>
