<primitives>
    <!-- 
    -  The following definitions describe the primitives available
    -  and their association to implementing Java classes.
    -
    -  NOTE: Only the tags "define" are used for initializing.
    -    The tags "Define" are pure comments waiting to be activated!
    -->
    <define name="fontvalue" 
        class="de.dante.extex.interpreter.primitives.font.FontValue"/>
    <define name="inputencoding" 
        class="de.dante.extex.interpreter.primitives.register.toks.NamedToks"> 
        ISO8859-1</define>
    <define name="inputfile" 
        class="de.dante.extex.interpreter.primitives.file.InputFile"/>
    <define name="inputfileencoding" 
        class="de.dante.extex.interpreter.primitives.file.InputFileEncoding"/>
    <Define name="setlanguage" 
        class="de.dante.extex.interpreter.primitives.hyphen.SetLanguage"/>
    <Define name="tosp" 
        class="de.dante.extex.interpreter.primitives.info.ToSP"/>
    <define name="printformat" 
        class="de.dante.extex.interpreter.primitives.info.PrintFormat"/>
</primitives>